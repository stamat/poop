{
  "serve" : {
    "port": 4040,
    "base": "/"
  },
  "livereload": true,
  "watch": [
    "src"
  ],
  "include_paths": [
    "node_modules"
  ],
  "script": {
    "in": "src/js/main.ts",
    "out": "dist/scripts.js"
  },
  "style": {
    "in": "src/scss/index.scss",
    "out": "dist/styles.css"
  }
}
